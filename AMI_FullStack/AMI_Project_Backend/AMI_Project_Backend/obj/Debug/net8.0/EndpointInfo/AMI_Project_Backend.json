  "components": {
    "schemas": {
      "ConsumerDto": {
        "required": [
          "createdAt",
          "name",
          "orgUnitId",
          "status",
          "tariffId"
        ],
        "type": "object",
        "properties": {
          "consumerId": {
            "type": "integer",
            "format": "int64"
          },
          "name": {
            "maxLength": 100,
            "minLength": 0,
            "pattern": "^[a-zA-Z\\s]+$",
            "type": "string"
          },
          "address": {
            "type": "string",
            "nullable": true
          },
          "phone": {
            "pattern": "^\\d{10}$",
            "type": "string",
            "nullable": true
          },
          "email": {
            "maxLength": 150,
            "minLength": 0,
            "type": "string",
            "format": "email",
            "nullable": true
          },
          "orgUnitId": {
